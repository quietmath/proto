{
  "name": "@quietmath/proto",
  "version": "1.1.0",
  "description": "Extension functions for JavaScript data types and objects.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/example/**/*.js",
      "**/example/**/*.ts",
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha ./test/*.test.js --ignore ./**/node_modules/**",
    "eslint": "eslint ./src/*.ts ./src/**/*.ts",
    "eslint-fix": "eslint ./src/*.ts ./src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quietmath/proto.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "strings",
    "arrays",
    "numbers",
    "objects"
  ],
  "author": "Michael Szul <michael@szul.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quietmath/proto/issues"
  },
  "homepage": "https://github.com/quietmath/proto#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^10.17.6",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "madge": "^3.9.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
